{
  "version": 2,
  "name": "yessales-admin",
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build@latest",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/admin",
      "dest": "/index.html"
    },
    {
      "src": "/admin/(.*)",
      "dest": "/index.html"
    },
    {
      "src": "/sales",
      "dest": "/index.html"
    },
    {
      "src": "/sales/(.*)",
      "dest": "/index.html"
    },
    {
      "src": "/test/(.*)",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "headers": {
        "Cache-Control": "public, max-age=0, must-revalidate"
      }
    }
  ],
  "installCommand": "cd frontend && npm ci",
  "buildCommand": "cd frontend && npm run build:h5",
  "outputDirectory": "frontend/dist",
  "framework": "vue"
}