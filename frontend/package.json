{
  "name": "yessales-frontend",
  "version": "1.0.1",
  "description": "YesSales（耶氏台球斗南销售中心报价系统）Frontend - Vue 3 + UniApp + Supabase",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 3001",
    "dev:h5": "vite --config vite.config.standard.ts --host 0.0.0.0 --port 3001",
    "dev:mp-weixin": "uni",
    "build": "vite build --config vite.config.standard.ts",
    "build:h5": "vite build --config vite.config.standard.ts",
    "build:mp-weixin": "uni build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .eslintignore",
    "format": "prettier --write src/",
    "validate": "npm run type-check && npm run lint && prettier --check src/",
    "preview": "vite preview --port 4173",
    "test:unit": "vitest",
    "test:smoke": "vitest run src/tests/"
  },
  "dependencies": {
    "vue": "^3.4.29",
    "pinia": "^2.1.7",
    "@supabase/supabase-js": "^2.43.4",
    "@vitejs/plugin-vue": "^5.2.4",
    "vite": "^5.4.19",
    "typescript": "^5.2.2",
    "vue-tsc": "^1.8.27",
    "sass": "^1.77.6",
    "sass-embedded": "^1.90.0"
  },
  "devDependencies": {
    "@dcloudio/types": "^3.4.19",
    "@uni-helper/uni-types": "^1.0.0-alpha.6",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.17.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "prettier": "^3.3.2",
    "vitest": "^1.6.0",
    "@vitejs/plugin-legacy": "^5.4.1",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.39"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "iOS >= 10",
    "Android >= 6"
  ],
  "keywords": [
    "vue3",
    "uniapp",
    "supabase",
    "typescript",
    "billiards",
    "sales",
    "quotation"
  ],
  "author": "YesSales Team",
  "license": "UNLICENSED",
  "private": true
}