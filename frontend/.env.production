# YesSales Admin - 生产环境配置
# ==========================================

# 应用基本信息
VITE_APP_NAME="YesSales Admin"
VITE_APP_VERSION="1.0.0"
VITE_BUILD_NUMBER=""
VITE_ENVIRONMENT="production"

# API 配置
VITE_API_BASE_URL="https://api.yessales.com/v1"
VITE_API_TIMEOUT="30000"
VITE_API_RETRY_ATTEMPTS="3"
VITE_API_MAX_CONCURRENT="10"

# Supabase 配置
VITE_SUPABASE_URL="https://your-project.supabase.co"
VITE_SUPABASE_ANON_KEY="your_production_anon_key_here"

# CDN 和静态资源
VITE_CDN_URL="https://cdn.yessales.com"
VITE_STATIC_URL="https://static.yessales.com"
VITE_IMAGE_OPTIMIZATION="true"
VITE_ENABLE_WEBP="true"

# 安全配置
VITE_ENABLE_HTTPS="true"
VITE_ENABLE_CSP="true"
VITE_SESSION_TIMEOUT="1800000"
VITE_MAX_LOGIN_ATTEMPTS="5"

# 功能开关
VITE_ENABLE_REAL_TIME="true"
VITE_ENABLE_OFFLINE_MODE="true"
VITE_ENABLE_VIRTUAL_SCROLLING="true"
VITE_ENABLE_ADVANCED_FILTERS="true"
VITE_ENABLE_PUSH_NOTIFICATIONS="false"
VITE_ENABLE_ANALYTICS="true"

# 性能配置
VITE_CACHE_MAX_SIZE="104857600"
VITE_CACHE_STRATEGY="networkFirst"
VITE_VIRTUAL_SCROLL_THRESHOLD="100"
VITE_MAX_UPLOAD_SIZE="10485760"

# 监控和日志
VITE_ENABLE_PERFORMANCE_TRACKING="true"
VITE_ENABLE_ERROR_TRACKING="true"
VITE_MONITORING_SAMPLE_RATE="0.1"
VITE_LOG_LEVEL="error"
VITE_ENABLE_REMOTE_LOGGING="true"

# 监控服务端点（实际项目中应该配置真实端点）
VITE_MONITORING_ENDPOINT=""
VITE_ERROR_REPORTING_ENDPOINT=""
VITE_ANALYTICS_ENDPOINT=""

# 第三方服务密钥（实际项目中应该从安全的密钥管理服务获取）
VITE_MONITORING_KEY=""
VITE_ANALYTICS_KEY=""

# 配置预设
VITE_CONFIG_PRESET="standard"

# 构建配置
VITE_ENABLE_BUNDLE_ANALYZER="false"
VITE_ENABLE_SOURCE_MAPS="false"
VITE_ENABLE_COMPRESSION="true"

# API 实际使用配置
VITE_USE_REAL_API="true"
VITE_ENABLE_API_CACHE="true"

# 国际化
VITE_DEFAULT_LOCALE="zh-CN"
VITE_SUPPORTED_LOCALES="zh-CN,en-US"