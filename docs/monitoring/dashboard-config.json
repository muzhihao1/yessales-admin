{
  "dashboards": [
    {
      "name": "系统概览",
      "type": "overview",
      "config": {
        "layout": "grid",
        "refreshInterval": 60,
        "widgets": [
          {
            "id": "system-health",
            "type": "status",
            "title": "系统健康状态",
            "position": { "x": 0, "y": 0, "w": 4, "h": 2 },
            "config": {
              "metrics": [
                { "source": "system", "name": "error_rate", "threshold": 5 },
                { "source": "system", "name": "api_response_time", "threshold": 2000 },
                { "source": "system", "name": "db_connections", "threshold": 90 }
              ]
            }
          },
          {
            "id": "api-performance",
            "type": "timeseries",
            "title": "API 响应时间",
            "position": { "x": 4, "y": 0, "w": 8, "h": 4 },
            "config": {
              "metric": { "source": "system", "name": "api_response_time" },
              "timeRange": "24h",
              "interval": "1h",
              "yAxis": { "unit": "ms", "label": "响应时间" }
            }
          },
          {
            "id": "error-rate",
            "type": "gauge",
            "title": "错误率",
            "position": { "x": 0, "y": 2, "w": 4, "h": 2 },
            "config": {
              "metric": { "source": "system", "name": "error_rate" },
              "unit": "%",
              "thresholds": [
                { "value": 1, "color": "green" },
                { "value": 3, "color": "yellow" },
                { "value": 5, "color": "red" }
              ]
            }
          },
          {
            "id": "active-alerts",
            "type": "alerts",
            "title": "活跃告警",
            "position": { "x": 0, "y": 4, "w": 12, "h": 3 },
            "config": {
              "showResolved": false,
              "limit": 10
            }
          }
        ]
      }
    },
    {
      "name": "业务分析",
      "type": "business",
      "config": {
        "layout": "grid",
        "refreshInterval": 300,
        "widgets": [
          {
            "id": "daily-quotes",
            "type": "number",
            "title": "今日报价单",
            "position": { "x": 0, "y": 0, "w": 3, "h": 2 },
            "config": {
              "metric": { "source": "business", "name": "daily_quotes" },
              "comparison": "yesterday",
              "format": "number"
            }
          },
          {
            "id": "active-users",
            "type": "number",
            "title": "活跃用户",
            "position": { "x": 3, "y": 0, "w": 3, "h": 2 },
            "config": {
              "metric": { "source": "business", "name": "active_users_hourly" },
              "comparison": "last_hour",
              "format": "number"
            }
          },
          {
            "id": "quotes-trend",
            "type": "timeseries",
            "title": "报价单趋势",
            "position": { "x": 6, "y": 0, "w": 6, "h": 4 },
            "config": {
              "metric": { "source": "business", "name": "daily_quotes" },
              "timeRange": "30d",
              "interval": "1d",
              "chartType": "bar"
            }
          },
          {
            "id": "product-stats",
            "type": "table",
            "title": "产品统计",
            "position": { "x": 0, "y": 2, "w": 6, "h": 4 },
            "config": {
              "query": "SELECT * FROM product_performance_view LIMIT 20",
              "refreshInterval": 600
            }
          }
        ]
      }
    },
    {
      "name": "技术监控",
      "type": "technical",
      "config": {
        "layout": "grid",
        "refreshInterval": 60,
        "widgets": [
          {
            "id": "db-performance",
            "type": "multi-metric",
            "title": "数据库性能",
            "position": { "x": 0, "y": 0, "w": 6, "h": 4 },
            "config": {
              "metrics": [
                { "source": "system", "name": "db_connections", "label": "连接数" },
                { "source": "system", "name": "db_query_time", "label": "查询时间" }
              ],
              "timeRange": "6h",
              "interval": "5m"
            }
          },
          {
            "id": "storage-usage",
            "type": "progress",
            "title": "存储使用",
            "position": { "x": 6, "y": 0, "w": 6, "h": 2 },
            "config": {
              "metric": { "source": "system", "name": "storage_usage_percent" },
              "thresholds": [
                { "value": 80, "color": "yellow" },
                { "value": 90, "color": "red" }
              ]
            }
          },
          {
            "id": "slow-queries",
            "type": "table",
            "title": "慢查询",
            "position": { "x": 0, "y": 4, "w": 12, "h": 4 },
            "config": {
              "function": "get_slow_queries",
              "params": { "duration_ms": 1000 },
              "columns": ["query", "calls", "mean_time", "max_time"]
            }
          }
        ]
      }
    }
  ],
  "defaultDashboard": "系统概览",
  "theme": {
    "dark": false,
    "colors": {
      "primary": "#3B82F6",
      "success": "#10B981",
      "warning": "#F59E0B",
      "danger": "#EF4444",
      "info": "#6B7280"
    }
  }
}